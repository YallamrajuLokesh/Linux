#Develop a shell script that accepts above created filename as argument & display its creation time and permissions of the file, on the standard output


#! /bin/bash
if [ $# -eq 0 ]         #shell script importing
then
echo "display does not exit"     #to display
else
ls -l $1> t1
x=`cut -d ' ' -f  1,6,7,8,9  t1`
echo $x
fi

#execution
#sh 2b.sh 2a.sh


#output
#-rw-rw-r-- Dec 14 14:59 2a.sh
